<!DOCTYPE html>
<html lang="en">
<head>
  <%- include ('./components/head') %>
  <link rel="stylesheet" href="./css/user.css">
</head>
<body>
  <%- include ('./components/loader') %>

  <div class="wrapper">
    <div class="navbar">
      <%- userImage %>
      
      <h1><%- username %></h1>
      
      <select class="guilds">
        <% servers.forEach((g) => { %>
          <option value="<%= g.id %>"><a href="/<%= g.id %>"><%= g.name %></a></option>
        <% }); %>
      </select>
    </div>

    <div class="center">
      <%- include ('./components/guildData') %>
    </div>

    <%- include ('./components/footer') %>

  </div>

  <%- include ('./components/particules') %>

  <script src="./js/user.js"></script>
</body>
</html>